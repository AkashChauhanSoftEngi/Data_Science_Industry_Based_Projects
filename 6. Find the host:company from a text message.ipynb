   "source": [
    "# find the host from the given text\n",
    "\n",
    "data = 'From stephen.marquard@uct.ac.za Sat Jan 5 09:14:16 2008'\n",
    "atpos = data.find('@')\n",
    "\n",
    "sppos = data.find(' ',atpos)\n",
    "\n",
    "host = data[atpos+1 : sppos]\n",
    "print (host)"
